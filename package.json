{
  "name": "open-related-file",
  "displayName": "Open Related File",
  "description": "Quickly open a file with the same name and a different extension",
  "version": "0.0.1",
  "publisher": "georgesg",
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "standard && npm run compile && node ./node_modules/vscode/bin/test",
    "standard": "standard"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [ "typescript" ]
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "eslint-plugin-typescript": "^0.12.0",
    "standard": "^12.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "typescript-eslint-parser": "^18.0.0",
    "vscode": "^1.1.21"
  }
}
